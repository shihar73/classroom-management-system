<section class="session1">
    <div class="limiter">
        <div class="container-login100" style="background-image: url('/images/bg-01.jpg');">
            <div class="arrow-position">
                <a href="/tutor/students"><i class='fas fa-angle-double-left ' style='font-size:25px'></i></a>
            </div>
            <div class="wrapper">
                <div class="profile-card js-profile-card">
                    <div class="profile-card__img">
                        <img src="/images/students-profile/{{student._id}}.jpg" alt="profile card">
                    </div>

                    <div class="profile-card__cnt js-profile-cnt">
                        <div class="profile-card__name">{{student.name}}</div>
                        <hr>
                        <div class="profile-card__txt"><span style='font-size:14px'>Class :
                            </span><strong>{{student.class}}</strong> </div>
                        <div class="profile-card__txt"><span style='font-size:14px'>Roll No :
                            </span><strong>{{student.no}}</strong>
                        </div>
                        <hr>
                        <div class="profile-card__txt"><strong>{{student.house}}</strong> House</div>
                        <div class="profile-card-loc">
                            <span class="profile-card-loc__icon">
                                <svg class="icon">
                                    <i style='font-size:24px' class='fas'>&#xf3c5;</i>
                                </svg>
                            </span>

                            <span class="profile-card-loc__txt" data-placeholder="&#xf191;">
                                {{student.place}}
                            </span>
                        </div>
                        <div class="profile-card__txt"><span style='font-size:14px'>Pin: </span>{{student.pin}}</div>

                        <br>
                        <div class="profile-card__txt"><span style='font-size:14px'>Mob :
                            </span><strong>{{student.number}}</strong></div>
                        <div class="profile-card__txt"><span style='font-size:14px'>Email :
                            </span><strong>{{student.email}}</strong></div>
                        <br>
                        <div class="container-login100-form-btn">
                            <a href="/tutor//students-assignments/{{student._id}}" class="cancel100-form-btn">Assignments</a>
                            <a href="" class="cancel100-form-btn">Exams</a>
                        </div>
                        <br>
                        <div class="container-login100-form-btn">
                            <a href="/tutor/edit-student/{{student._id}}" class="edit100-form-btn">Edit</a>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="dropDownSelect1"></div>
</section>